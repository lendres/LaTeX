@article{ref:dong2016a,
	author			= {Dong, Guangjian and Chen, Ping},
	title			= {A Review of the Evaluation, Control, and Application Technologies for Drill String Vibrations and Shocks in Oil and Gas Well},
	journal			= SHOCKANDVIB,
	volume			= {2016},
	number			= {},
	day				= {},
