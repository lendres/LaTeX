				<tagprocessor xsi:type="StringReplacementTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="FEA"
					replacement="{FEA}">
					<tags>
						<tag>title</tag>
						<tag>abstract</tag>
					</tags>
				</tagprocessor>

				<tagprocessor xsi:type="StringReplacementTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="FEM"
					replacement="{FEM}">
					<tags>
						<tag>title</tag>
						<tag>abstract</tag>
					</tags>
				</tagprocessor>

				<tagprocessor xsi:type="StringReplacementTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="CFD"
					replacement="{CFD}">
					<tags>
						<tag>title</tag>
						<tag>abstract</tag>
					</tags>
				</tagprocessor>

				<tagprocessor xsi:type="StringReplacementTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="FPSO"
					replacement="{FPSO}">
					<tags>
						<tag>title</tag>
						<tag>abstract</tag>
					</tags>
				</tagprocessor>

				<tagprocessor xsi:type="StringReplacementTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="BHA"
					replacement="{BHA}">
					<tags>
						<tag>title</tag>
						<tag>abstract</tag>
					</tags>
				</tagprocessor>

				<tagprocessor xsi:type="StringReplacementTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="PDC"
					replacement="{PDC}">
					<tags>
						<tag>title</tag>
						<tag>abstract</tag>
					</tags>
				</tagprocessor>

				<tagprocessor xsi:type="StringReplacementTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="RSS"
					replacement="{RSS}">
					<tags>
						<tag>title</tag>
						<tag>abstract</tag>
					</tags>
				</tagprocessor>

				<tagprocessor xsi:type="StringReplacementTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="RWD"
					replacement="{RWD}">
					<tags>
						<tag>title</tag>
						<tag>abstract</tag>
					</tags>
				</tagprocessor>

			</tagprocessors>
		</tagprocessorgroup>

		<tagprocessorgroup name="Sepcialized Processors">
			<tagprocessors>
				
				<!-- Do not modify the pattern unless you really know what you are doing. -->
				<tagprocessor xsi:type="SentanceEndingSpacesTagProcessor"
					tagstoprocess="OnlySpecified"
					pattern="\.+[a-zA-Z]+"
					frenchspacing="false">
					<tags>
						<tag>abstract</tag>
					</tags>
					<exceptions>
						<exception>A.K.A.</exception>
						<exception>D.C.</exception>
						<exception>E.g.</exception>
						<exception>e.g.</exception>
						<exception>I.e.</exception>
						<exception>i.e.</exception>
						<exception>Ph.D.</exception>
						<exception>U.S.</exception>
						<exception>U.S.A.</exception>
					</exceptions>
				</tagprocessor>

				<!-- Remove enclosing brackets. -->
				<tagprocessor xsi:type="RemoveEnclosingBracesTagProcessor"
					tagstoprocess="All"
					pattern="^{[\s\S]*}$">
					<tags></tags>
				</tagprocessor>

				<!-- Change titles from UPPPER CASE to Title Case. -->
				<tagprocessor xsi:type="StringCaseTagProcessor" tagstoprocess="OnlySpecified" pattern="^[A-Z\s\p{P}]*$" tocase="TitleCase">
					<tags><tag>title</tag></tags>
				</tagprocessor>

				<!-- Change quotes to LaTeX quotes. -->
				<tagprocessor xsi:type="QuoteTagProcessor" tagstoprocess="All" pattern="&quot;[^&quot;]*&quot;">
					<tags></tags>
				</tagprocessor>
				
			</tagprocessors>
		</tagprocessorgroup>
		
	</tagprocessorgroups>
</qualityprocessor>